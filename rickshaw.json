{
    "rickshaw-utility": {
        "schema": {
            "version": "2021.06.16"
        }
    },
    "utility": "packrat",
    "controller": {
        "post-script": "%utility-dir%packrat-post-process"
    },
    "engine": {
        "files-from-controller": [
